{
  "name": "route-v",
  "version": "0.2.0",
  "repository": "git@github.com:Amri91/route-v.git",
  "description": "Route version control library.",
  "main": "index.js",
  "scripts": {
    "test": "npm run test:coverage && npm run test:lint",
    "test:coverage": "jest tests --coverage -i --forceExit",
    "test:lint": "eslint src",
    "docs": "jsdoc2md -t jsdoc2md/readme.hbs lib.js > readme.md",
    "ci": "npm run test:lint && npm run test:coverage && codecov"
  },
  "author": "Abdulrahman Amri",
  "license": "MIT",
  "dependencies": {
    "ramda": "latest",
    "semver": "^5.4.1",
    "tcomb": "^3.2.24"
  },
  "devDependencies": {
    "eslint": "^4.13.1",
    "eslint-config-warp": "^2.1.0",
    "jest": "^22.0.4",
    "jsdoc-to-markdown": "^3.0.3",
    "codecov": "^3.0.0"
  },
  "keywords": [
    "versioning",
    "route",
    "routes",
    "router",
    "semver",
    "middleware",
    "koa",
    "express"
  ]
}
